// Простой пример для тестирования
var x = 10;
var y = 5;
var sum = x + y;
print("Сумма: " + sum);

function add(a, b) {
    return a + b;
}

print("Результат функции: " + add(3, 4));

var age = 18;
if (age >= 18) {
    print("Совершеннолетний");
} else {
    print("Несовершеннолетний");
}

var i = 1;
while (i <= 5) {
    print("Итерация: " + i);
    i = i + 1;
}

print("Скрипт выполнен успешно!");
