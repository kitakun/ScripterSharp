// Simple example for testing
var x = 10;
var y = 5;
var sum = x + y;
print("Sum: " + sum);

function add(a, b) {
    return a + b;
}

print("Function result: " + add(3, 4));

var age = 18;
if (age >= 18) {
    print("Adult");
} else {
    print("Minor");
}

var i = 1;
while (i <= 5) {
    print("Iteration: " + i);
    i = i + 1;
}

print("Script executed successfully!");
